    <%= form_for(@user) do |f| %>
      <%= f.label :name ,"ユーザー名" %>
      <%= f.text_field :name %>

      <%= f.label :email , "メールアドレス" %>
      <%= f.text_field :email %>
      
      <%= f.label :password , "パスワード" %>
      <%= f.text_field :password %>
      
      <%= f.label :password_confirmation, "パスワード(確認)" %>
      <%= f.text_field :password_confirmation %>
      
      <%= f.submit %>
    <% end %>